---
import Layout from '../layouts/Layout.astro';
import Footer from '../components/Footer.astro';

export const prerender = false;
---

<Layout title="Order Labs">

  <main class="flex-1 flex items-center justify-center">
    <div class="container mx-auto px-4 py-16">
      <div class="text-center max-w-2xl mx-auto">
        <h1 class="text-4xl font-bold mb-6">Order Labs</h1>
        <p class="text-lg text-base-content/70 mb-8">
          Lab ordering functionality coming soon. We're working on partnerships with lab providers to bring you convenient, affordable lab testing.
        </p>
        <a href="/" class="btn btn-outline">Back to Home</a>
      </div>
    </div>
  </main>

  <Footer />
</Layout>